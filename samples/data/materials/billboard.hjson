shader: shaders/simple_billboard.hjson
blend_mode: {
  BlendMode: {
    colour_blend_op: add
    alpha_blend_op: add
    src_colour_factor: one
    dst_colour_factor: zero
    src_alpha_factor: one
    dst_alpha_factor: zero
  }
}
options: {
  A_TEST: true
  FADE_WHEN_CLOSE: false
  FIXED_SIZE: false
  SHRINK_WHEN_CLOSE: true
}
parameters: {
  sampler_diffuse: {
    type: sampler2D
    value: textures/light_t.dds
  }
}
