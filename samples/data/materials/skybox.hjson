shader: shaders/skybox.hjson
blend_mode: {
  BlendMode: {
    colour_blend_op: add
    alpha_blend_op: add
    src_colour_factor: one
    dst_colour_factor: zero
    src_alpha_factor: one
    dst_alpha_factor: zero
  }
}
parameters: {
  sampler_diffuse: {
    type: samplerCube
    value: textures/cubemap/test_sky_d.dds
  }
  exposure: {
    type: float
    value: 1.0
  }
}
